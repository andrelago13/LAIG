    <LIGHT id="right3">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="23.9" y="7" z="5" w="1" />       <!-- light position -->
        <ambient r="0.2" g="0.2" b="0.2" a="1" />        <!-- ambient component -->
        <diffuse r="0.3" g="0.3" b="0.3" a="1" />        <!-- diffuse component -->
        <specular r="0.3" g="0.3" b="0.3" a="1" />       <!-- specular component -->
    </LIGHT>

    <!-- NOTE: this block "LIGHT" must be repeated as necessary with different "id" -->
    
</LIGHTS>


<TEXTURES>
    <TEXTURE id="floor">
        <file path="textures/blue_fabric.jpg" />                             <!-- path to file -->
        <amplif_factor s="0.1" t="0.1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="seats_floor">
        <file path="textures/blue_fabric.jpg" />                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="wall">
        <file path="textures/black_fabric.jpg" />                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="wall_seamless">
        <file path="textures/seamless_black_fabric.jpg" />                             <!-- path to file -->
        <amplif_factor s="2" t="2" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="rolo-projetor">
        <file path="textures/roll.png" />                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="screen-img">
        <file path="textures/screen.jpg" />                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="door">
        <file path="textures/door.png" />                             <!-- path to file -->
        <amplif_factor s="2" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="metal">
        <file path="textures/metal.jpg" />                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="projector-roll-tape">
        <file path="textures/tape.png" />                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="projector-roll-tape2">
        <file path="textures/tape2.png" />                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="projector-lens">
        <file path="textures/lens.png" />                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="projector-pannel">
        <file path="textures/projector.png" />                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    
    <TEXTURE id="projector-material">
        <file path="textures/projector-material.jpg" />                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>

    <!-- NOTE: this block "TEXTURE" must be repeated as necessary with different "id" -->
    
</TEXTURES>


<MATERIALS>
    <MATERIAL id="material_metalico">                                
        <shininess value="100" />
        <specular r="0.5" g="0.5" b="0.5" a="1" />        <!-- specular reflection -->
        <diffuse r="0.5" g="0.5" b="0.5" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.8" g="0.8" b="0.8" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="1" />        <!-- emission component -->
    </MATERIAL>
    
    <MATERIAL id="safety-bar">                                
        <shininess value="20" />
        <specular r="0.8" g="0.1" b="0.1" a="1" />        <!-- specular reflection -->
        <diffuse r="0.2" g="0.025" b="0.025" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.2" g="0.025" b="0.025" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="1" />        <!-- emission component -->
    </MATERIAL>
    
    <MATERIAL id="floor">                                
        <shininess value="50" />
        <specular r="0" g="0" b="0" a="1" />        <!-- specular reflection -->
        <diffuse r="0.7" g="0.7" b="0.7" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.2" g="0.2" b="0.2" a="1" />         <!-- ambient reflection --> <!-- Lower than diffuse to create a better visual effect -->
        <emission r="0" g="0" b="0" a="1" />        <!-- emission component -->
    </MATERIAL>
    
    <MATERIAL id="screen">                                
        <shininess value="5" />
        <specular r="0.05" g="0.05" b="0.05" a="1" />        <!-- specular reflection -->
        <diffuse r="0.9" g="0.9" b="0.9" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.9" g="0.9" b="0.9" a="1" />         <!-- ambient reflection -->
        <emission r="0.5" g="0.5" b="0.5" a="1" />        <!-- emission component -->
    </MATERIAL>
    
    <MATERIAL id="sofa">                                
        <shininess value="2" />
        <specular r="0.05" g="0.05" b="0.05" a="1" />        <!-- specular reflection -->
        <diffuse r="0.1" g="0.1" b="0.1" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.05" g="0.05" b="0.05" a="1" />         <!-- ambient reflection --> <!-- Lower than diffuse to create a better visual effect -->
        <emission r="0" g="0" b="0" a="1" />        <!-- emission component -->
    </MATERIAL>
    
    <MATERIAL id="bright-metal">                                
        <shininess value="20" />
        <specular r="1" g="1" b="1" a="1" />        <!-- specular reflection -->
        <diffuse r="0.2" g="0.2" b="0.2" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.2" g="0.2" b="0.2" a="1" />         <!-- ambient reflection --> <!-- Lower than diffuse to create a better visual effect -->
        <emission r="0" g="0" b="0" a="1" />        <!-- emission component -->
    </MATERIAL>
    
    <MATERIAL id="projector-lens">                                
        <shininess value="200" />
        <specular r="1" g="1" b="1" a="1" />        <!-- specular reflection -->
        <diffuse r="0.2" g="0.2" b="0.2" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.2" g="0.2" b="0.2" a="1" />         <!-- ambient reflection --> <!-- Lower than diffuse to create a better visual effect -->
        <emission r="1" g="1" b="1" a="1" />        <!-- emission component -->
    </MATERIAL>

    <!-- NOTE: the "MATERIAL" block may be repeated as required. Each defined material requires a distinct "id" -->

</MATERIALS>


<!-- The leaves -->
<LEAVES>
    <LEAF id="rectangle" type="rectangle" args="-0.5 0.5 0.5 -0.5" />
    <LEAF id="cylinder" type="cylinder" args="1 1 1 5 20" />
    <LEAF id="circle" type="cylinder" args="0 1 0 1 20" />
    <LEAF id="sphere" type="sphere" args="1 20 20" />
    <LEAF id="triangle" type="triangle" args="0 0 0 0 0 1 0 1 0" />
    <LEAF id="triangle2" type="triangle" args="0 0 1 0 0 0 0 1 0" />
</LEAVES>


