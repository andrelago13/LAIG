<SCENE>

<INITIALS>
    <frustum near="0.1" far="500.0"/>                      	<!-- frustum planes-->
    <translation x="0.0" y="0.0" z="0.0" />                 	<!-- initial translate -->
    <rotation axis="x" angle="0.0" />                  		<!-- initial rotation 3 -->
    <rotation axis="y" angle="0.0" />                 		<!-- initial rotation 2 -->
    <rotation axis="z" angle="0.0" />                  		<!-- initial rotation 1 -->
    <scale sx="1" sy="1" sz="1" />                  	<!-- initial scaling -->
    <reference length="5" />                          		<!-- axis length; "0" means no axis-->
</INITIALS>


<ILLUMINATION>
    <ambient r="0.05" g="0.05" b="0.05" a="1" />            <!-- global ambient -->
    <background r="0" g="0" b="0.1" a="1" />         <!-- background color -->
</ILLUMINATION>


<LIGHTS>
    <LIGHT id="screen">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="12" y="4" z="23.1" w="1" />       <!-- light position -->
        <ambient r="0.2" g="0.2" b="0.2" a="1" />        <!-- ambient component -->
        <diffuse r="0.2" g="0.2" b="0.2" a="1" />        <!-- diffuse component -->
        <specular r="0" g="0" b="0" a="1" />       	<!-- specular component --> <!-- no specular here because it's a wide source of light -->
    </LIGHT>
    
    <!-- Ambients set to a value lower than diffuse to create a better visual effect -->
    
    <LIGHT id="left1">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="0.1" y="7" z="18" w="1" />       <!-- light position -->
        <ambient r="0.2" g="0.2" b="0.2" a="1" />        <!-- ambient component -->
        <diffuse r="0.3" g="0.3" b="0.3" a="1" />        <!-- diffuse component -->
        <specular r="0.3" g="0.3" b="0.3" a="1" />       <!-- specular component -->
    </LIGHT>
    
    <LIGHT id="left2">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="0.1" y="7" z="11.5" w="1" />       <!-- light position -->
        <ambient r="0.2" g="0.2" b="0.2" a="1" />        <!-- ambient component -->
        <diffuse r="0.3" g="0.3" b="0.3" a="1" />        <!-- diffuse component -->
        <specular r="0.3" g="0.3" b="0.3" a="1" />       <!-- specular component -->
    </LIGHT>
    
    <LIGHT id="left3">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="0.1" y="7" z="5" w="1" />       <!-- light position -->
        <ambient r="0.2" g="0.2" b="0.2" a="1" />        <!-- ambient component -->
        <diffuse r="0.3" g="0.3" b="0.3" a="1" />        <!-- diffuse component -->
        <specular r="0.3" g="0.3" b="0.3" a="1" />       <!-- specular component -->
    </LIGHT>
    
    <LIGHT id="right1">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="23.9" y="7" z="18" w="1" />       <!-- light position -->
        <ambient r="0.2" g="0.2" b="0.2" a="1" />        <!-- ambient component -->
        <diffuse r="0.3" g="0.3" b="0.3" a="1" />        <!-- diffuse component -->
        <specular r="0.3" g="0.3" b="0.3" a="1" />       <!-- specular component -->
    </LIGHT>
    
    <LIGHT id="right2">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="23.9" y="7" z="11.5" w="1" />       <!-- light position -->
        <ambient r="0.2" g="0.2" b="0.2" a="1" />        <!-- ambient component -->
        <diffuse r="0.3" g="0.3" b="0.3" a="1" />        <!-- diffuse component -->
        <specular r="0.3" g="0.3" b="0.3" a="1" />       <!-- specular component -->
